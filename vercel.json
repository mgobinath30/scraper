{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/app.js" },
       { "src": "/out(.*)", "dest": "/out" },
       { "src": "/templates/wordpress.html(.*)", "dest": "/templates/wordpress.html" }
    ]
 }

 